{
  "extends": "astro/tsconfigs/base",
  "moduleResolution": "node",
  "compilerOptions": {
    "baseUrl": ".",
    "resolveJsonModule": true,
    "types": [
      "vite/client",
      "astro/client"
    ],
    "typeRoots": [
      "./node_modules/@types"
    ],
    "importsNotUsedAsValues": "error",
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "paths": {
      "@/*": [
        "src/*"
      ],
      "@components/*": [
        "src/components/*"
      ],
      "@layouts/*": [
        "src/layouts/*"
      ],
      "@data/*": [
        "src/data/*"
      ],
      "@utils/*": [
        "src/utils/*"
      ]
    }
  }
}